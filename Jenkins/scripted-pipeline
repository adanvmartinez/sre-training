pipeline{
  stages{
    stage('Build'){
      if(isUnix()){
        def os = sh script: 'uname', returnStdout: true
        if(is.startsWith('Darwin')){
          echo 'Running in MacOS'
        }
        else{
          echo 'Running on Linux'
        }
      }
      else{
        echo 'Running in Windows'
      }
    }
  }
}
